pipeline {
        agent any
        stages {
          stage('Build') {
            steps{
              sh 'mvn clean install'
                }
                }
          stage("build & SonarQube Scanner") {
            agent any
            steps {
              withSonarQubeEnv('SonarQube_Server') {
                sh 'mvn clean package sonar:sonar'
              }
            }
          }
        }
      }
